#Environment for UID=root

#Source this to run/debug/buid for host as root with shared libraries
#Source from main project directory.
export SAMPLER_ENV_SET='y'
#export LIB_DYNAMIC='y'
# If running as root, did you build as root+ If so, the line below is
# not necessary
#export LD_LIBRARY_PATH=$(pwd)/libmtime/lib
unset LIB_DYNAMIC
unset  KDBG_SESSION
